{
  "name": "Usuario",
  "plural": "Usuarios",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "nombre": {
      "type": "string"
    },
    "localizacion": {
      "type": "GeoPoint"
    },
    "edad": {
      "type": "number"
    }
  },
  "validations": [],
  "relations": {
    "juegos": {
      "type": "hasMany",
      "model": "Juego",
      "foreignKey": "idUsuario",
      "through": "JuegoUsuario",
      "keyThrough": "idJuego"
    },
    "juegosUsuario": {
      "type": "hasMany",
      "model": "JuegoUsuario",
      "foreignKey": "idUsuario"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
