{
  "name": "Juego",
  "plural": "Juegos",
  "base": "PersistedModel",
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "id": {
      "type": "number",
      "id": true,
      "required": true
    },
    "nombre": {
      "type": "string",
      "required": "true"
    },
    "ano": {
      "type": "date"
    },
    "resumen": {
      "type": "date"
    },
    "urlImagen": {
      "type": "string"
    },
    "valoracionIgdb": {
      "type": "number"
    },
    "popularidadIgdb": {
      "type": "number"
    },
    "fechaInsercion": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "usuarios": {
      "type": "hasMany",
      "model": "Usuario",
      "foreignKey": "idJuego",
      "through": "JuegoUsuario",
      "keyThrough": "idUsuario"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property":"igdb"
    }
  ],
  "methods": {}
}
